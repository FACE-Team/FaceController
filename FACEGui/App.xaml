<Application x:Class="FACEGui20.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Button.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Checkbox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Combobox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Expander.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Menu.xaml"/>
