                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Radiobutton.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Scrollbar.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Scrollviewer.xaml"/>                
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/TabPanel.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Textbox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Toolbar.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AppResources;component/Resources/Tooltip.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
